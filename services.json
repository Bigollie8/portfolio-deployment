{
  "version": "1.0",
  "domain": "basedsecurity.net",
  "awsRegion": "us-east-1",
  "ec2": {
    "host": "98.88.74.174",
    "user": "ec2-user"
  },
  "discord": {
    "enabled": true
  },
  "services": {
    "portfolio": {
      "type": "frontend",
      "repo": "Bigollie8/terminal-portfolio",
      "path": "frontend",
