      "testCommand": "npm test",
      "buildCommand": "npm run build",
      "distDir": "dist",
      "s3Bucket": "portfolio-prod-portfolio-frontend",
      "cloudFrontId": "E15VHZ20NOJ0RE",
      "url": "https://portfolio.basedsecurity.net",
      "healthCheck": "https://portfolio.basedsecurity.net"
    },
    "portfolio-backend": {
      "type": "backend",
      "repo": "Bigollie8/terminal-portfolio",
      "path": "backend",
      "runtime": "node",
      "testCommand": "npm test",
      "dockerImage": "portfolio-backend",
      "containerName": "portfolio-backend",
      "healthCheck": "https://api.basedsecurity.net/health"
    },
    "photos": {
      "type": "frontend",
      "repo": "OliverLand0/rapidPhotoFlow",
      "path": "frontend",
